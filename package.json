{
  "name": "syno-nuxt3",
  "version": "1.0.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "nuxt prepare && nuxt dev",
    "build": "nuxt build --prerender && electron-builder --win --publish never",
    "deploy": "nuxt build --prerender && electron-builder --win --publish always",
    "deploy:patch": "npm version patch --force && npm run deploy",
    "deploy:minor": "npm version minor --force && npm run deploy",
    "deploy:major": "npm version major --force && npm run deploy",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.4.6",
    "@types/humanize-duration": "^3.27.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18",
    "electron": "^24.1.3",
    "electron-builder": "^23.6.0",
    "nuxt": "^3.4.3",
    "nuxt-electron": "^0.4.5",
    "vite-electron-plugin": "^0.8.2",
    "vite-plugin-electron-renderer": "^0.14.1"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.10",
    "@vueuse/core": "^10.1.2",
    "@vueuse/electron": "^10.1.2",
    "chart.js": "^4.3.0",
    "electron-log": "^4.4.8",
    "electron-settings": "^4.0.2",
    "electron-updater": "^5.3.0",
    "filesize": "^10.0.7",
    "lodash": "^4.17.21",
    "pinia": "^2.0.36",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.29.1",
    "sass": "^1.62.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "tiny-cookie": "^2.4.1",
    "vue-json-pretty": "^2.2.4"
  }
}
